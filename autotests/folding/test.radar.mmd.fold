radar-beta
  title Restaurant Comparison
  axis food["Food Quality"], service["Service"], price["Price"]
  axis axis1, axis2, axis3
  curve id1["Label1"]<beginfold id='1'>{</beginfold id='1'>1, 2, 3<endfold id='1'>}</endfold id='1'>
  curve id2["Label2"]<beginfold id='1'>{</beginfold id='1'>4, 5, 6<endfold id='1'>}</endfold id='1'>, id3<beginfold id='1'>{</beginfold id='1'>7, 8, 9<endfold id='1'>}</endfold id='1'> %% two curves
  curve id4<beginfold id='1'>{</beginfold id='1'> axis3: 30, axis1: 20, axis2: 10 <endfold id='1'>}</endfold id='1'>

  %% options
  graticule polygon
  max 5
  showLegend true
